<h3>{{title}}</h3>
<h5>Created By: {{username}}</h4>
<div class ='container '>
    <div class='row align-self-start'>
        <div class='col-6'>
           
        <div class='list-group '>
             {{#each pages}}
            <a href='/page/{{this.slug}}' class='list-group-item clearfix list-group-item-action flex-column align-items-start'>
                 <div class ='d-flex w-100 justify-content-between'>
                    <h5 class='mb-1'> {{this.page_name}} </h5>
                    <small> {{this.date}}</small>
                </div>
                <p class='mb-1'>{{this.notes.content}}</p>

            </a>
             {{/each}}
        </div>
       
        </div>
</div>
</div>
